<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="1dip" android:color="#146c5c" />
            <gradient android:angle="-90" android:startColor="#4bae9e" android:endColor="#689a92"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#1bae93" />
            <solid android:color="#099c85"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="2dip" />
            <stroke android:width="1dip" android:color="#127e6a" />
            <gradient android:angle="-90" android:startColor="#8dbab3" android:endColor="#107969" />
        </shape>
    </item>
</selector>